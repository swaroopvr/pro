

<div class="table-view">
	<table>
		<caption>LIST OF PULSE_WIDTH COMMANDS</caption>
		<thead>
			<tr>
				<th> ID </th>
				<th> PULSE_WIDTH </th>

			</tr>
		</thead>
		<tbody>
		<% @pulse_width.each do |f| %>
		<tr data-href="<%= edit_pulse_width_tbl_path(f) %>"  class="theShower text-center">
				<td><%= f.ID  %></td>
				<td><%= f.PULSE_WIDTH%></td>
				<% if current_user.admin? %>	<!-- add this too -->
					<td class="deleter"><%= link_to "x", f , method: :delete, data: { confirm: "You sure?" } %></td>	<!-- add this too -->
				<% end %>
			
		</tr>	
		<% end %>
		</tbody>
	</table>

	</div>
	<%= link_to 'Add', new_pulse_width_tbl_path, class: "add-new-table" %>