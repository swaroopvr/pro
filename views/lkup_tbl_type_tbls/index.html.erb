
<div class="table-view">
	<table>
		<caption>LIST OF BIT_SEL COMMANDS</caption>
		<thead>
			<tr>
				<th> ID </th>
				<th> LKUP_TBL_TYPE </th>

			</tr>
		</thead>
		<tbody>
		<% @lkup_type.each do |f| %>
		<tr data-href="<%= edit_bit_sel_tbl_info_path(f) %>"  class="theShower text-center">
	
		
		<td><%= f.ID %></td>
				<td><%= f.LKUP_TBL_TYPE %></td>
				<% if current_user.admin? %>	<!-- add this too -->
					<td class="deleter"><%= link_to "x", f , method: :delete, data: { confirm: "You sure?" } %></td>	<!-- add this too -->
				<% end %>
			
		</tr>	
		<% end %>
		</tbody>
	</table>

	</div>
	<%= link_to 'Add', new_lkup_tbl_type_tbl_path, class: "add-new-table" %>