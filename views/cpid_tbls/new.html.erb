
<h1>CREATE CPID_TBL </h1>

<div class="perfect-forms">
<%= form_for(@cpids) do |f| %>

  <%= f.hidden_field :CMD_ID, :value => @base_cmds.CMD_ID %>

<div class="form-row">
  <%= f.label :CPID %>
  <%= f.text_field :CPID, class: 'form-control' %>
  </div>

<div class="form-row">
  <%= f.label :CPID_DESC%>
  <%= f.text_field :CPID_DESC, class: 'form-control' %>
</div>

<div class="form-row">
  <%= f.label :TM_VAL %>
  <%= f.text_field :TM_VAL, class: 'form-control' %>
</div>

<div class="form-row">
  <%= f.label :TM_COUNT %>
  <%= f.text_field :TM_COUNT, class: 'form-control' %>
</div>

<div class="form-row">
  <%= f.label :PROC_TYPE%>
  <%= f.text_field :PROC_TYPE, class: 'form-control' %>
</div>

  <div class="form-row">
  <%= f.submit 'Create', class: "btn btn-primary" %>
  </div>
<% end %>
