
<h1>CREATE CONFIRMER_TBL </h1>
<div class="perfect-forms">
<%= form_for(@confs) do |f| %>
<%= render 'shared/error_messages', object: f.object %>

<div class="form-row">
  <%= f.label :POSITION %>
  <%= f.text_field :POSITION, class: 'form-control' %>
  </div>

<div class="form-row">
  <%= f.label :NAME %>
  <%= f.text_field :NAME, class: 'form-control' %>
  </div>

<div class="form-row">
  <%= f.submit 'Create', class: "btn btn-primary" %>
  </div>
<% end %>
