


<div class="table-view">
	<table>
		<caption>LIST OF REL_OP  COMMANDS</caption>
		<thead>
			<tr>
				<th> ID </th>
				<th> REL_OP  </th>

			</tr>
		</thead>
		<tbody>
		<% @rel_op.each do |f| %>
		<tr data-href="<%= edit_rel_op_tbl_path(f) %>"  class="theShower text-center">
	
		
		<td><%= f.ID  %></td>
				<td><%= f.REL_OP %></td>
				<% if current_user.admin? %>	<!-- add this too -->
					<td class="deleter"><%= link_to "x", f , method: :delete, data: { confirm: "You sure?" } %></td>	<!-- add this too -->
				<% end %>
			
		</tr>	
		<% end %>
		</tbody>
	</table>

	</div>
	<%= link_to 'Add', new_rel_op_tbl_path, class: "add-new-table" %>